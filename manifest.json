{
	"name": "YouTube Live to Google Calendar",
	"short_name": "YouTubeLive2Calendar",
	"version": "1.4.2",
	"manifest_version": 2,
	"description": "リマインダーを設定したYouTubeライブの予定をGoogleカレンダーに追加します。",
	"icons": {
		"128": "icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.youtube.com/*"
			],
			"js": [
				"jquery-3.4.1.min.js",
				"content.js"
			]
		}
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"storage"
	],
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	}
}